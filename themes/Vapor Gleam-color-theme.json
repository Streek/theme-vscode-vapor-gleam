    "contrastBorder": "#ffc1eb"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6c7086",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator.new",
        "keyword.other.import",
        "keyword.other.package"
      ],
      "settings": {
        "foreground": "#c4a7e7",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "variable",
        "variable.other",
        "variable.language.this"
      ],
      "settings": {
        "foreground": "#ffc1eb"
      }
    },
    {
      "scope": [
        "variable.parameter",
        "meta.parameter",
        "entity.name.variable.parameter"
      ],
      "settings": {
        "foreground": "#e4c87b",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "string.quoted",
        "string.template",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#7dd3fc"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#e4c87b"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call entity.name.function"
      ],
      "settings": {
        "foreground": "#8bbec7"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.type"
      ],
      "settings": {
        "foreground": "#ffc1eb"
      }
    },
    {
      "scope": [
        "entity.name.class",
        "entity.other.attribute-name",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#c4a7e7"
      }
    },
    {
      "scope": [
        "punctuation",
        "meta.brace",
        "meta.delimiter",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#ffc1eb"
      }
    },
    {
      "scope": [
        "support.type",
        "support.class",
        "support.constant"
      ],
      "settings": {
        "foreground": "#ffc1eb"
      }
    },
    {
      "scope": [
        "entity.name.module",
        "entity.name.namespace"
      ],
      "settings": {
        "foreground": "#c4a7e7"
      }
    },
    {
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#e0def4"
      }
    },
    {
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#e0def4"
      }
    },
    {
      "scope": [
        "markup.underline"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#6c7086",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code"
      ],
      "settings": {
        "foreground": "#f2cb7c"
      }
    },
    {
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#ffc1eb",
        "fontStyle": "bold underline"
      }
    },
    {
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#8bbec7"
      }
    },
    {
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#c4a7e7"
      }
    },
    {
      "scope": [
        "meta.import entity.name",
        "meta.import variable.other"
      ],
      "settings": {
        "foreground": "#8bbec7"
      }
    },
    {
      "scope": [
        "source.gleam entity.name.function",
        "source.gleam meta.function"
      ],
      "settings": {
        "foreground": "#8bbec7"
      }
    },
    {
      "scope": [
        "source.gleam keyword",
        "source.gleam storage.type"
      ],
      "settings": {
        "foreground": "#c4a7e7",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "source.gleam string"
      ],
      "settings": {
        "foreground": "#7dd3fc"
      }
    },
    {
      "scope": [
        "source.gleam comment"
      ],
      "settings": {
        "foreground": "#6c7086",
        "fontStyle": "italic"
      }
    }
  ]
}